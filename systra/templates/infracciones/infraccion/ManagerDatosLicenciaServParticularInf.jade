#cont-se-recoje-licencia
	.controls.controls-row
		.span4
			label Estado:
			input#estado_dl_sp_inf.ui-autocomplete-input(type="text",autocomplete="off", role="textbox",arial-autocomplete="list" ,aria-haspopup="true")
		.span4
			label Lic. No. Conducir:
			input#no_licencia_dl_sp_inf(type="text",autocomplete="off", role="textbox",arial-autocomplete="list" ,aria-haspopup="true")
		.span4
			.control-group
				label Fecha Vencimiento:
				#fecha-venc-inf.input-append.date
					input#fecha_vencimiento_dl_sp_inf.input_medium(type="text",autocomplete="off", role="textbox",arial-autocomplete="list" ,aria-haspopup="true")
					span.add-on
						i(data-time-icon="icosystra-clock", data-date-icon="icosystra-clock")

	.controls.controls-row	
		.span4
			label Tipo: 
			select#tipo_licencia_dl_sp_inf.input-medium
					option(value="NO") **SELECCIONAR**
					option(value="A") A
					option(value="B") B
					option(value="C") C
					option(value="D") D
					option(value="PERMISO") PERMISO
					option(value="FEDERAL") FEDERAL
					option(value="EXTRANJERA") EXTRANJERA
					option(value="PERMANENTE") PERMANENTE
		.span4
			label Restricciones:
			select#restriciones_licencia_dl_sp_inf
					option(value="NO") **SELECCIONAR**
					option(value="Anteojos") Anteojos
					option(value="Protesis") Protesis
					option(value="Hipertensión") Hipertensión
					option(value="Diabetis") Diabetis
					option(value="Ninguna") Ninguna 
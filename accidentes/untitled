  create table Inventario_Vehiculo1(
    folio_inventario   int(11),
    folio_infraccion   int(11),
    folio_accidente    int(11),
    delegacion  varchar(255),
    comandancia  varchar(255),
    turno  varchar(255),
    tipo_servicio varchar(255),
    folio_agentes date,
    fecha_incid date,
    fecha_captura date,
    fecha_modifica date,
    fecha_salida date, 
    pension_hora time,
    pension_fecha date
    calle1 varchar(255),
    calle2 varchar(255),  
    colonia varchar(255),
    interior varchar(255), 
    exterior varchar(255), 
    depositado_en varchar(255), 
    clas_vehiculo varchar(255),
    marca varchar(255), 
    submarca varchar(255),
    veh_modelo varchar(255),
    veh_color varchar(255),
    veh_color_detalle varchar(255),
    veh_placas varchar(255),
    veh_serie varchar(255),
    infractor_nombre varchar(255),
    infractor_apepaterno varchar(255),
    infractor_apematerno varchar(255),
    pension_lugar varchar(255), 
    pension_recibido_por varchar(255),
    pension_transportado_por varchar(255),
    pension_transportado_pension varchar(255),
    pension_transportado_unidad_transito varchar(255),
    usuario varchar(255),
    salida_nombre varchar(255),
    salida_apepaterno varchar(255),
    salida_apematerno varchar(255),
    liberacion_fecha date,
    liberacion_entrega varchar(255),
    liberacion_recibe varchar(255),
    agencia_mp varchar(255),
    folio_consignacion varchar(255),
    activo int(11),
    fecha_entrega date
    corporacion varchar(255)
    );
